<script>
  document.addEventListener("DOMContentLoaded", function() {
    var openHelpWallet = document.querySelector('.open_help_wallet');
    var closeHelpWallet = document.querySelector('.close_help_wallet');
    var detailDiv = document.querySelector('.total_ballane_detail_div');
    var helpDiv = document.querySelector('.total_ballane_detail_help_div');

    openHelpWallet.addEventListener('click', function() {
        detailDiv.style.display = 'none';
        helpDiv.style.display = 'block';
    });

    closeHelpWallet.addEventListener('click', function() {
        detailDiv.style.display = 'block';
        helpDiv.style.display = 'none';
    });
});
</script>
<script>
   function openTab(tabId) {
       // Get all tab content elements
       var tabContents = document.querySelectorAll('.tab-content');
       // Hide all tab content elements
       tabContents.forEach(function(content) {
           content.classList.remove('active');
       });
       // Show the selected tab content
       document.getElementById(tabId).classList.add('active');
       // Get all tab elements
       var tabs = document.querySelectorAll('.tab');
       // Remove the 'active' class from all tabs
       tabs.forEach(function(tab) {
           tab.classList.remove('active');
       });
       // Add the 'active' class to the clicked tab
       event.currentTarget.classList.add('active');
   }
   // Initially open the first tab
   // window.onload = function() {
   //   openTab('tab1');
   // };
 </script>



