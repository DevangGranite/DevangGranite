  font-weight: 400;
  line-height: 18px;
}
.payment_status_cancel{
  color: var(--Alert-Red-Alert, #DF1515);
  font-feature-settings: 'clig' off, 'liga' off;
  font-family: Rubik;
  font-size: 14px;
  font-style: normal;
  font-weight: 400;
  line-height: 18px;
}
.order_history_main{
  display: flex;
  width: 1000px;
  flex-direction: column;
  justify-content: center;
  padding-top: 50px;
}
.order_history_main h1{
    color: #001489;
    text-align: center;
    font-family: Rubik !important;
    font-size: 32px;
    font-style: normal;
    font-weight: 400 !important;
    line-height: 34px;
}
.order_history_main th{
    color: #001489;
    font-family: Rubik;
    font-size: 14px;
    font-style: normal;
    font-weight: 500;
    line-height: 18px;
}
.rwd-table {
  margin: auto;
  min-width: 300px;
  max-width: 100%;
  border-collapse: collapse;
}

.rwd-table tr:first-child {
  border-top: none;
  background: #EFF2F6;
  color: #001489;
}

.rwd-table tr {
  border-top: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
  background-color: #EFF2F6;
}

.rwd-table tr:nth-child(odd):not(:first-child) {
  background-color: #EFF2F6;
}

{% comment %} .rwd-table th {
  display: none;
} {% endcomment %}

.rwd-table td {
  /* display: block; */
}

.rwd-table td:first-child {
  margin-top: .5em;
}

.rwd-table td:last-child {
  margin-bottom: .5em;
}

.rwd-table td:before {
  {% comment %} content: attr(data-th) ": ";
  font-weight: bold;
  width: 120px;
  display: inline-block;
  color: #000; {% endcomment %}
}

.rwd-table th,
.rwd-table td {
  /* text-align: left; */
}

.rwd-table {
  color: #333;
  border-radius: .4em;
  overflow: hidden;
}

.rwd-table tr {
  border-color: #bfbfbf;
}

.rwd-table th,
.rwd-table td {
  padding: .5em 1em;
}
@media screen and (max-width: 601px) {
  .rwd-table tr:nth-child(2) {
    border-top: none;
  }
}
@media screen and (min-width: 600px) {
  .rwd-table tr:hover:not(:first-child) {
    background-color: #d8e7f3;
  }
  .rwd-table td:before {
    display: none;
  }
  .rwd-table th,
  .rwd-table td {
    display: table-cell;
    padding: .25em .5em;
  }
  .rwd-table th:first-child,
  .rwd-table td:first-child {
    padding-left: 0;
  }
  .rwd-table th:last-child,
  .rwd-table td:last-child {
    padding-right: 0;
  }
  .rwd-table th,
  .rwd-table td {
    padding: 1em !important;
  }
}


